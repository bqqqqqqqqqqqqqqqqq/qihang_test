<van-cell-group title="{{type}}列表"></van-cell-group>
<van-cell-group wx:for="{{userList}}" wx:key="id">
  <van-field
    border="{{ false }}"
    use-button-slot
    label="{{item.name}}"
    readonly
  >
    <van-button slot="button" size="small" type="danger" 
    data-name="{{item.name}}" data-id="{{item.id}}"
    bind:click="click">
      移除
    </van-button>
  </van-field>
</van-cell-group>

<van-dialog id="van-dialog" />